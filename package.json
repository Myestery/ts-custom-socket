{
  "name": "ts-custom-socket",
  "version": "1.0.0",
  "type": "module",
  "description": "A private extension of socket.io ",
  "main": "index.js",
  "scripts": {
    "clean": "rm dist/bundle.js",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "test": "jest",
    "dev": "ts-node src/index.ts",
    "build": "tsc --build",
    "watch": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"nodemon --delay 500ms ./js/index.js\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Myestery/ts-custom-socket.git"
  },
