  "bugs": {
    "url": "https://github.com/Myestery/ts-custom-socket/issues"
  },
  "homepage": "https://github.com/Myestery/ts-custom-socket#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "nodemon": "^2.0.19",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "jest": "^28.1.3",
